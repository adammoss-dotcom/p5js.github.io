	@yannmartins
	@teddavis
	modified Boid class to ES5
*/

let personaggini = []

function setup() {
	createCanvas(1920, 1000)
	textAlign(CENTER, CENTER)
	var loopc = innerWidth / 10
	var brdr = 20
	personaggini = []
	for(var i = 0; i < loopc; i++) {
		personaggini.push(new Boid(random(brdr, width - brdr), random(brdr, height - brdr)))
	}

}

function draw() {
	clear()
	for(let boid of personaggini) {
		boid.update()
		boid.proximity(personaggini)
		boid.edges()
		boid.show(personaggini)
	}
}

function mousePressed() {
	personaggini.push(new Boid(mouseX, mouseY))
}

function keyPressed() {
	if(keyCode == 8) {
		personaggini = []
	}

}


