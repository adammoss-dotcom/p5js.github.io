]

"use strict";

function _classCallCheck(instance, Constructor) {
	if(!(instance instanceof Constructor)) {
		throw new TypeError("Cannot call a class as a function")
	}
}

var Boid = (function() {
	function Boid(x, y) {
		_classCallCheck(this, Boid)

		this.boid = random(emojis)
		this.pos = createVector(x, y)
		this.ppos = createVector(this.pos.x, this.pos.y)
		this.rvel = 1.5
		this.vel = createVector(
			random(-this.rvel, this.rvel),
			random(-this.rvel, this.rvel)
		)
		this.r = random(15, 45)
		this.directionx = 1
		this.directiony = 1
		this.maxSpeed = 50
		this.trail = []
	}

	Boid.prototype.show = function show(balls) {
		this.proximity(balls)
		textSize(this.r)
		text(this.boid, this.pos.x, this.pos.y)
	};

	Boid.prototype.update = function update() {
		this.pos.x += this.vel.x * this.directionx
		this.pos.y += this.vel.y * this.directiony
	};

	Boid.prototype.edges = function edges() {
		if(this.pos.x > width - this.r / 2 || this.pos.x < this.r / 2) {
			this.directionx *= -1
		}
		if(this.pos.y > height - this.r / 2 || this.pos.y < this.r / 2) {
			this.directiony *= -1
		}
	};

	Boid.prototype.proximity = function proximity(balls) {
		for(
			var _iterator = balls,
				_isArray = Array.isArray(_iterator),
				_i = 0,
				_iterator = _isArray ? _iterator : _iterator[Symbol.iterator]();;

		) {
			var _ref

			if(_isArray) {
				if(_i >= _iterator.length) break;
				_ref = _iterator[_i++];
			} else {
				_i = _iterator.next();
				if(_i.done) break;
				_ref = _i.value;
			}

			var ball = _ref

			if(ball != this) {
				if(ball.pos.dist(this.pos) < 50) {
					strokeWeight(2)
					stroke(255)
					line(ball.pos.x, ball.pos.y, this.pos.x, this.pos.y)
					if(this.r < 60) this.r += 0.1
					var dir = p5.Vector.sub(this.pos, ball.pos)
					dir.normalize()
					if(
						this.pos.y < height - this.r / 2 &&
						this.pos.y > this.r / 2 &&
						this.pos.x < width - this.r / 2 &&
						this.pos.x > this.r / 2
					)
						this.pos.add(dir)
					this.ppos = this.pos
				}
			}
		}
	};

	return Boid
})()
